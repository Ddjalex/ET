; ==========================================
; Ethiopian Proxy Configuration (Optional)
; ==========================================
; Enable proxy for fetching TeleBirr, CBE, M-Pesa receipts
; This is useful if receipt URLs are geo-restricted to Ethiopia

; Enable/disable proxy (true/false)
PROXY_ENABLED="false"

; Proxy server details
; Example: proxy.example.com or 123.45.67.89
PROXY_HOST=""
PROXY_PORT="8080"

; Proxy type: HTTP, SOCKS4, or SOCKS5
PROXY_TYPE="HTTP"

; Proxy authentication (leave empty if no auth required)
; Format: username:password
PROXY_AUTH=""

; Auto-fallback to direct connection if proxy fails
PROXY_AUTO_FALLBACK="true"

; Fallback proxy list (semicolon-separated)
; Format: host:port|type|username:password
; Example: proxy1.et:8080|HTTP|user:pass;proxy2.et:1080|SOCKS5
PROXY_FALLBACK_LIST=""

; Health check URL for proxy testing
PROXY_HEALTH_URL="https://www.google.com"

; Allowed domains for receipt verification
ALLOWED_DOMAINS="transactioninfo.ethiotelecom.et,apps.cbe.com.et,www.combanketh.et,mpesa.et,safaricom.et,m-pesa.et,mpesa.ethiotelecom.et"

; Application timezone
APP_TZ="Africa/Addis_Ababa"
